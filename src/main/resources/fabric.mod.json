{
	"schemaVersion": 1,
	"id": "custommod",
	"version": "${version}",
	"name": "CustomMod",
	"description": "This is Victor Avila's first Minecraft mod made for learning purposes",
	"authors": [
		"Victor Avila"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/custommod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.victor.custommod.CustomMod"
		],
		"fabric-datagen": [
			"com.victor.custommod.CustomModDataGenerator"
		],
		"client" : [
			"com.victor.custommod.CustomModClient"
		]
	},
	"mixins": [
		"custommod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.6",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}